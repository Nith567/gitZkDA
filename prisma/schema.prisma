generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model Quest {
    id              String   @id @default(uuid())
    createdAt       DateTime @default(now())
    contractAddress String   @unique
    theme           String
    entryCost       String
    scoreToWin      Int
}
